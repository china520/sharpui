    <!-- Window控件样式定义 -->
    <Style TargetType="Window">
        <Setter Property="Background" Value="{mainbkgnd}" />
        <Setter Property="RoundSize" Value="0" />
        <Setter Property="BorderThickness" Value="0"/>
        <Style.Triggers>
            <Trigger Property="IsInactivate" Value="True">
                <Setter Property="Background" Value="{mainbkgnd}" />
                <Setter Property="BorderThickness" Value="1,1,1,2"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <!-- ToolTip控件样式定义 -->
    <Style TargetType="ToolTip">
        <Setter Property="Font1" Value="{ftooltip}" />
        <Setter Property="Background" Value="{tooltip}" />
        <Setter Property="Foreground" Value="#666666"/>
        <Setter Property="Padding" Value="4,4,4,6" />
    </Style>
    
    <Style TargetType="TitleMenu" >
        <Setter Property="Background" Value="{normal_menu}" />
        <Setter Property="Margin" Value="0" />
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{hot_menu}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="{down_menu}" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Menu" >
        <Setter Property="BorderThickness" Value="2" />
        <Setter Property="Padding" Value="1" />
        <Setter Property="Background" Value="{menu_bk}" />
    </Style>
    
    <Style TargetType="MenuItem">
        <Setter Property="Background1" Value="#66FFF1" />
        <Setter Property="BorderBrush" Value="#888888"/>
        <Setter Property="Foreground" Value="#000000"/>
        <Setter Property="Padding" Value="2" />
        <Style.Triggers>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Background" Value="#66FFF1" />
            </Trigger>
            <Trigger Property="IsDisabled" Value="True">
                <Setter Property="Foreground" Value="#AAAAAA" />
            </Trigger>
        </Style.Triggers>
    </Style>
    
</ResourceDictionary>
